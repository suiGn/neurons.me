{
  "source": {
    "include": ["./src", "./index.js"]
  },
  "opts": {
    "destination": "./docs",
    "template": "node_modules/better-docs"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
    "default": {
        "staticFiles": {
          "include": [
              "./README.md"
          ]
        }
    },
    "better-docs": {
        "name": "neurons.me",
        "logo": "_._.svg",
        "title": "neurons.me", 
        "css": "style.css",
        "trackingCode": "tracking-code-which-will-go-to-the-HEAD",
  "hideGenerator": false,
        "navLinks": [
            {
                "label": "Github",
                "href": "https://github.com/suiGn/neurons.me"
            },
            {
                "label": "App",
                "href": "https://neurons.me"
            }
        ]
    }
}
}